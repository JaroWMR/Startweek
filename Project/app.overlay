//enable used

&usart1 {
		pinctrl-0 = < &usart1_tx_pb7 &usart1_rx_pb6 >;
		pinctrl-names = "default";
        current-speed = <9600>;
        status = "okay";
        gnss: gnss-nmea-generic {
                compatible = "gnss-nmea-generic";
        };
};

&usart2 {
		pinctrl-0 = < &usart2_tx_pa2 &usart2_rx_pa3 >;
		pinctrl-names = "default";
        current-speed = <115200>;
        status = "okay";
};


&i2c1{
		status = "okay";
		pinctrl-0 = < &i2c1_scl_pb8 &i2c1_sda_pb9 >;
		gyro: lis3mdl-magn@1e {
		compatible = "st,lis3mdl-magn";
		reg = <0x1E>;
		irq-gpios = <&gpiob 8 GPIO_ACTIVE_HIGH>;
		};
};

&timers1{
	status = "okay";
	pwm1: pwm{
		compatible = "st,stm32-pwm";
		status = "okay";
		#pwm-cells = < 0x3 >;
		pinctrl-0 = < &tim1_ch1_pe9 &tim1_ch2_pe11 &tim1_ch3_pe13 &tim1_ch4_pe14>;
		pinctrl-names = "default";
	};
};

&timers12{
	status = "disabled";
	pwm{
		status = "disabled";
	};
};
