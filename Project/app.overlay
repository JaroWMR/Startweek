			};

			//enable and configure pins
			usb_otg_fs_dm_pa11: usb_otg_fs_dm_pa11 {
				pinmux = < 0x16a >;
				phandle = < 0x22 >;
				status = "okay";
			};
			usb_otg_fs_dp_pa12: usb_otg_fs_dp_pa12 {
				pinmux = < 0x18a >;
				phandle = < 0x23 >;
				status = "okay";
			};
			usart1_rx_pb6: usart1_rx_pb6 {
				pinmux = <STM32_PINMUX('B', 6, AF7)>;
				status = "okay";
			};
			usart1_tx_pb7: usart1_tx_pb7 {
				pinmux = <STM32_PINMUX('B', 7, AF7)>;
				bias-pull-up;
				status = "okay";
			};
			usart2_rx_pb3: usart2_tx_pa3 {
				pinmux = <STM32_PINMUX('a', 3, AF7)>;
				status = "okay";
			};
			usart2_tx_pb2: usart2_tx_pa2 {
				pinmux = <STM32_PINMUX('a', 2, AF7)>;
				bias-pull-up;
				status = "okay";
			};
			i2c1_scl_pb8: i2c1_scl_pb8 {
				pinmux = < 0x304 >;
				bias-pull-up;
				drive-open-drain;
				status = "okay";
				phandle = < 0xb >;
			};
			i2c1_sda_pb9: i2c1_sda_pb9 {
				pinmux = < 0x324 >;
				bias-pull-up;
				drive-open-drain;
				status = "okay";
				phandle = < 0xc >;
			};
			adc1_inp2_pf11: adc1_inp2_pf11 {
				pinmux = <STM32_PINMUX('f', 11, AF10)>;
				status = "okay";
			};
			tim1_ch1_pe9: tim1_ch1_pe9 {
				pinmux = <STM32_PINMUX('e', 9, AF1)>;
				status = "okay";
			};
			tim1_ch2_pe11: tim1_ch2_pe11 {
				pinmux = <STM32_PINMUX('e', 11, AF1)>;
				status = "okay";
			};
			tim1_ch3_pe13: tim1_ch3_pe13 {
				pinmux = <STM32_PINMUX('e', 13, AF1)>;
				status = "okay";
			};
			tim1_ch4_pe14: tim1_ch4_pe14 {
				pinmux = <STM32_PINMUX('e', 14, AF1)>;
				status = "okay";
			};
		};
