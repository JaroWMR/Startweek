#include <zephyr/dt-bindings/gpio/gpio.h>

/ {
    gpioCustom {
        compatible = "gpio-keys";
        D2: gpioa_10 {
            gpios = <&gpioa 10 GPIO_PULL_UP | GPIO_ACTIVE_LOW>;
            label = "gpio D2";
        };
        D3: gpiob_3 {
            gpios = <&gpiob 3 GPIO_PULL_UP | GPIO_ACTIVE_LOW>;
            label = "gpio D3";
        };
