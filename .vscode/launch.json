{ 
    "version": "0.2.0", 
    "configurations": [ 
        { 
            "name": "Flash and Debug using J-Link",
            "device": "${config:bsp.soc}",
            "type": "cortex-debug", 
            "cwd": "../",
            "executable": "${workspaceFolder:Project}/build/zephyr/zephyr.elf",
            "request": "launch", 
            "servertype": "jlink", 
            "gdbPath": "../zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "breakAfterReset": true, 
            "runToEntryPoint": "main",
			"preLaunchTask": "West Build App"
        },
		{ 
            "name": "Debug using J-Link",
            "device": "${config:bsp.soc}",
            "type": "cortex-debug", 
            "cwd": "../",
            "executable": "${workspaceFolder:Project}/build/zephyr/zephyr.elf",
            "request": "launch", 
            "servertype": "jlink", 
            "gdbPath": "../zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "breakAfterReset": true, 
            "runToEntryPoint": "main",            
        },
        {
            "name": "Attach using J-Link",
            "device": "${config:bsp.soc}",
            "type": "cortex-debug",
            "cwd": "../",
            "executable": "${workspaceFolder:Project}/build/zephyr/zephyr.elf",
            "request": "attach",
            "servertype": "jlink",
            "gdbPath": "../zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "breakAfterReset": true, 
            "runToEntryPoint": "main",
        }, 
    ], 
}